{
  "name": "streamlit",
  "version": "1.37.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "start": "pnpm --filter app start",
    "build": "pnpm -r --workspace-concurrency=1 build",
    "buildLib": "pnpm --filter lib build",
    "buildLibProd": "pnpm --filter lib build:prod",
    "buildApp": "pnpm --filter app build",
    "typecheck": "pnpm -r --workspace-concurrency=1 run typecheck",
    "lint": "pnpm -r run lint",
    "lintApp": "pnpm --filter app run lint",
    "lintLib": "pnpm --filter lib run lint",
    "prettier": "prettier --write --config .prettierrc app/src && prettier --write --config .prettierrc lib/src",
    "test": "pnpm -r run test",
    "testLib": "pnpm --filter lib run testWatch",
    "testApp": "pnpm --filter app run testWatch",
    "cy:open": "unset NODE_OPTIONS && cypress open --env devicePixelRatio=1",
    "cy:run": "unset NODE_OPTIONS && cypress run"
  },
  "resolutions": {
    "nwsapi": "2.2.9"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
